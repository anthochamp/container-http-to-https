{
	"private": true,
	"scripts": {
		"test": "node --import=tsx --test-concurrency 1 --test tests/**/*.test.*",
		"tsc:check": "tsc -noEmit -p ./tsconfig.json",
		"biome:check": "biome check --error-on-warnings --no-errors-on-unmatched --files-ignore-unknown=true",
		"biome:fix": "biome check --error-on-warnings --no-errors-on-unmatched --files-ignore-unknown=true --write",
		"prepare": "lefthook install"
	},
	"devDependencies": {
		"@types/node": "22.5.5",
		"@biomejs/biome": "1.9.1",
		"tsx": "4.19.1",
		"find-up": "7.0.0",
		"typescript": "5.6.2",
		"lefthook": "1.7.15"
	}
}
